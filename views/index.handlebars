            </div>
            <div class="clear"></div>
        </div>
        <div class="banner">

            <div class="section group">
                <div class="cont span_2_of_c" id="leftPanel">

                    <div class="hidden" id="hidden">
                        <div class="web_card">
                            <div class="delete_icon hidden" style="position:relative" >
                                <img src="images/delete_icon.png"  type="click" width = "20px" height="20px" style="position:absolute; right:0px;top:0px" >
                            </div>
                            <div class="desc">
                                <h3><a href="{{mainSite}}">{{siteName}}</a></h3>
                                <ul>
                                    {{#each subSites}}
                                        <div class="web_card_subSites">
                                            <a href="{{url}}">
                                                {{title}}
                                            </a>
                                        </div>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="hidden" id="hidden">
                        <div class="web_card">
                            <div class="delete_icon hidden" style="position:relative" >

                                <img src="images/delete_icon.png"  type="click" width = "20px" height="20px" style="position:absolute; right:0px;top:0px">
                            </div>
                            <div class="desc">
                                <h3><a href="{{mainSite}}">{{siteName}}</a></h3>
                                <ul>
                                    {{#each subSites}}
                                        <div>
                                            <a href="{{url}}">
                                                {{title}}
                                            </a>
                                        </div>
                                    {{/each}}
                                </ul>
                            </div>
                        </div>
                    </div>

                    {{#each cards}}
                        <div class="lsidebar" id="{{mainSite}}">
                            <div class="web_card">
                                <div class="delete_icon hidden" style="position:relative" >

                                    <img src="images/delete_icon.png"  type="click" width = "20px" height="20px" style="position:absolute; right:0px;top:0px">
                                </div>
                                <div class="desc">
                                    <h3><a href="{{mainSite}}">{{siteName}}</a></h3>
                                    <ul>
                                        {{#each subSites}}
                                            <div>
                                                <a href="{{url}}">
                                                    {{title}}
                                                </a>
                                            </div>
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                        </div>
                    {{/each}}

                    <!--<div class="clear"></div>-->

                    <!--<div class="clear"></div>-->

                    <!--<div class="clear"></div>-->
                </div>
                <div class="rsidebar span_1_of_c">
                    <div class="head">
                        <h2>News & Events</h2>
                    </div>
                    <div class="right-box">
                        {{#each lists}}
                            <div class="right-box-grid">
                                <div class="right-box-grid-img">
                                    <a href="#"><img src="images/pic8.jpg" title="img"></a>
                                </div>
                                <div class="right-box-grid-title">
                                    <h5><a href="{{mainSite}}">{{siteName}}</a></h5>
                                    <p>{{title}}</p>
                                </div >
                                    <img class="right-box-grid-button hidden" src="images/add_icon.png" type="button" style="float: left; position:relative; left: 0px; top: -2px;">
                                <div class="clear"> </div>
                            </div>
                        {{/each}}
                    </div>
                    <div>
                        <text><a href="/">Show More ...</a></text>
                    </div>
                </div>

                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
</div>
